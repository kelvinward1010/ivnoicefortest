{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -p tsconfig.build.json && vite build",
    "lint": "biome check --apply-unsafe --no-errors-on-unmatched --files-ignore-unknown=true ./",
    "lint:staged": "biome check --write --staged --files-ignore-unknown=true --no-errors-on-unmatched",
    "preview": "vite preview",
    "generate-client": "openapi-ts",
    "i18n:extract": "i18next src/**/*.tsx src/**/**/*.tsx src/**/**/**/*.tsx src/**/*.ts src/**/**/*.ts src/**/**/**/*.ts --config i18next-parser.config.json"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.1.1",
    "@chakra-ui/react": "^3.4.0",
    "@emotion/react": "^11.14.0",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@tanstack/react-query": "^5.28.14",
    "@tanstack/react-query-devtools": "^5.28.14",
    "@tanstack/react-router": "^1.97.3",
    "axios": "1.7.4",
    "date-fns": "^4.1.0",
    "form-data": "4.0.0",
    "framer-motion": "^12.5.0",
    "i18next": "^24.2.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-parser": "^9.1.0",
    "libphonenumber-js": "^1.11.19",
    "next-themes": "^0.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.49.3",
    "react-i18next": "^15.4.0",
    "react-icons": "^5.5.0",
    "react-phone-input-2": "^2.15.1",
    "react-select": "^5.10.0",
    "react-toastify": "^11.0.3",
    "use-debounce": "^10.0.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@hey-api/openapi-ts": "^0.57.0",
    "@playwright/test": "^1.45.2",
    "@tanstack/router-devtools": "1.19.1",
    "@tanstack/router-vite-plugin": "1.19.0",
    "@types/node": "^20.17.17",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "dotenv": "^16.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.13"
  }
}
